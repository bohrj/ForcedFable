o:Sass::Tree::RootNode:@template"d"@import url(http://fonts.googleapis.com/css?family=Ubuntu:400,500|Oxygen:400,300|Nothing+You+Could+Do);

$red: #d12727;
$grey: #343434;

*
{
	transition: all .3s ease-in-out;
}

html
{
  font-family: Oxygen, sans-serif;
  font-size: 100%;
  font-weight: 300;
  
  //background-image: url("../img/witewall_3.png");
  //background-attachment: fixed;
}

body
{
  line-height: 1.5em;
}

div.container
{
  padding: 60px 0px;
  
  margin: 0 auto;
  max-width: 1020px;
  width: 95%;
}

h1,h2,h3,h4,h5,h6
{
  margin: 0px;
  padding: 0px;
  line-height: 1.4em;
  font-weight: 400;
}

h1
{
  font-family: Ubuntu, sans-serif;
  font-size: 1.8em;
}

h3
{
  font-size: 1.25em;
}

p
{
	margin-top: 5px;
}

img
{
	max-width: 100%;
}

code
{
	border-bottom: 1px solid BROWN;
}

a
{
	color: $red;
	
	
	&:hover
	{
		i.icon-dribbble
		{
			color: #ff0084;
		}
		
		i.icon-facebook
		{
			color: #3b5998;
		}
		
		i.icon-twitter
		{
			color: #00aced;
		}
		
		i.icon-forrst
		{
			color: #4fb64b;
		}
	}
}

small
{
  font-size: .6em;
}

header
{
  position: fixed;
  z-index: 1;
  top: 0;
  left: 0;
  right: 0;
  
  height: 50px;
  width: 100%;
  
  background-color: $grey;
  
  img
  {
    float: left;
    display: inline-block;
    
    margin: 10px;
    
    border-radius: 50%;
    box-shadow: 0px 0px 3px rgba(50, 50, 50, 0.75);
    
    border: 2px solid #FFF;
    
    -webkit-transition: all .2s ease-in-out;
    
    &:hover
    {
      //margin-left: 50px;
      -webkit-transform: scale(1.2) rotate(33deg);
    }
  }
}

a.menu-toggle
{
	position: absolute;
	top: 0;
	left: 0; right: 0;

	display: none;
	width: 100%;
	line-height: 50px;
	
	text-align: center;
	text-decoration: none;
	
	font-size: 2em;
	background-color: $grey;
	color: WHITE;
}

nav
{
  float: right;
  display: inline-block;
}

ul.nav
{
  list-style-type: none;
  margin: 0;
  
  li
  {
    display: inline-block;
    
    padding: 0 10px;
    min-height: 50px;
    
    &:hover
    {
      background-color: darken($grey, 10);
    }
    
    a
    {
      display: block;
      
      color: #FFF;
      text-decoration: none;
      line-height: 50px;
      
      -webkit-font-smoothing: antialiased;
    }
  }
}

.main_content
{
  position: relative;
}

.articles
{ 
  float: left;
  width: 70%;
}

.sidebar
{
  float: right;
  
  width: 25%;
  
  h3
  {
	  font-family: 'Nothing You Could Do', sans-serif;
	  font-size: 1.5em;
  }
  
  p
  {
      
      img
      {
      	float: left;
      	margin-right: 5px;
      	
      	border-radius: 50%;
      	opacity: 1;
      	
        -webkit-transition: border-radius .5s ease-in-out;
      
        &:hover
        {
        	border-radius: 0;
        	opacity: .8;
        }
      
    }
  }
}
  
div.widget
{

	h4
	{
		font-family: 'Nothing You Could Do', sans-serif;
	}

	ul
	{
		list-style-type: none;
		padding-left: 0;
		
		li
		{
		  box-shadow: 0 3px 3px -4px $grey;
		  overflow: hidden;
		  
		  &:first-of-type
		  {
		  	border-top: 1px solid lighten($grey, 60);
		  }
		  
		  a
		  {      
		    display: block;
		    width: 100%;
		    
		    padding: 10px 0px;
		    
		    text-decoration: none;
		    
		    -webkit-transition: all .2s ease-in-out;
		    
		    &:hover
		    {
		      padding-left: 40px;
		    }
		  }
		}
	}
}

ul.social
{

	display: table;
	list-style-type: none;
	margin-left: -40px;
	
	width: 100%;
	li
	{
		display: table-cell;
		margin: auto;
		
		text-align: center;
		
		$height: 50px;
		background-color: rgba(218,207,191, .5);
		
		height: $height;
		
		border-left: 1px solid darken(#dacfbf, 1);
		
		-webkit-transition: all .2s linear;
		
		&:hover, &:active
		{
			background-color: rgba(218,207,191, 1)
		}
		
		&:first-of-type
		{
			border-left: none;
			
			border-top-left-radius: 5px;
			border-bottom-left-radius: 5px;
		}
		
		&:last-of-type
		{
			border-top-right-radius: 5px;
			border-bottom-right-radius: 5px;
		}
		
		a
		{
			display: block;
			height: 100%;
			line-height: $height;
			color: lighten($grey, 20);
			font-size: 1.3em;
			text-decoration: none;
			
			&:hover i
			{
				-webkit-transition: all .2s ease-in-out;
			}
			
		}
	}
}

article
{
  margin-bottom: 50px;
  
  padding: 5px 10px;
  
  h1
  {
	  margin: auto;
  
	  a
	  {
	    text-decoration: none;
	    
	    &:hover
	    {
	      text-decoration: underline;
	    }
	  }
  }
  
  img
  {
  
  	  &.center
  	  {
	  	  display: block;
	  	  margin: 0 auto;
  	  }
	  padding: 5px;
	  
	  box-shadow: 0 0 3px $grey;
  }
  
  a.btn
  {
    float: right;
  }
  
  p
  {
    margin-top: 5px;
  }
}

div.featured-post
{

	margin-bottom: 20px;
	
	& > small
	{
		color: $red;
		font-family: 'Nothing You Could Do', sans-serif;
	}
	
	hr
	{
		color: $grey;
		opacity: .5;
	}
}

a.work-header
{
	font-family: 'Nothing You Could Do', sans-serif;
	color: $grey;
	text-decoration: none;
}

article.work
{
	position: relative;
	
	a.thumb
	{
		position: relative;
	
		display: table-cell;
		
		background-color: rgba(255,255,255,0.5);
		
		//box-shadow: 0px 0px 3px lighten($grey, 20);
		text-decoration: none;
		
		-webkit-transition: all .3s linear;
		
		&:hover
		{
			background-color: #FFF;
		}
		
		img
		{
			width: 200px;
			
			padding: 0px;
			
			box-shadow: none;
		}
		
		span.subtitle
		{
			position: absolute;
			bottom: -.5em;
			left: 0px;
			
			display: block;
			width: 100%;
			
			background-color: rgba($grey, .9);
			
			text-align: center;
			
			font-size: .8em;
			color: white;
			
			font-style: italic;
		}
	}
	
	.work-image
	{
		display: block;
		margin: 0 auto;
	}
}

div.technology
{
	margin-bottom: 10px;

	background-color: #FFF;
	padding: 15px;
	
	border-radius: 3px;
	
	box-shadow: 0 0 2px lighten($grey, 20);
	
	a
	{
		text-decoration: none;
		
		&:hover
		{
			color: $grey;
		}
		
		h2
		{
			border-bottom: 1px dashed lighten($grey, 60);
			margin-bottom: 15px;
		}
	}
}

a.btn
{
  display: inline-block;
  border-radius: 3px;
  background-color: $red;
  color: #FFF;
  padding: 10px 15px;
  text-decoration: none;
  font-size: .9em;
  text-shadow: none;
  
  border-bottom: 3px solid darken($red, 20);
  
  -webkit-font-smoothing: antialiased;
    
  -webkit-transition: all .5s ease-in-out;
  
  &:hover, &:active
  {
    background-color: darken($red, 10);
  }
  
  &:active
  {
	  border: none;
  }
}

div.hero
{
  position: relative;
  
  width: 100%;
  height: 250px;
  
  margin-bottom: 20px;
  
  border-radius: 3px;
  
  background-image: url("../img/hero.jpg");
  background-size: cover;
}

div.blurb
{
  position: absolute;
  z-index: 0;
  
  top: 20px;
  bottom: 20px;
  left: 20px;
  right: 50%;
  
  width: 45%;
  
  overflow: hidden;
  
  padding: 15px;
  background-color: rgba(0,0,0,.5);
  color: #FFF;
  text-shadow: 0 0 1px #000;
  
  border-radius: 5px;
  
  h3
  {
	  font-family: 'Nothing You Could Do', sans-serif;
	  font-size: 1.5em;
  }
  
  a.btn
  {
    position: absolute;
    right: 10px;
    bottom: 10px;
    float: right;
  }
}

footer
{
	clear: both;
	
	font-family: 'Nothing You Could Do', sans-serif;
	font-size: .8em;
	
	color: $grey;
	text-align: center;
	
	ul.nav
	{
		margin-left: -40px;
		li
		{
			
			a
			{
				font-size: 1.3em;
				color: $red;
				
				&:hover
				{
					color: $grey;
				}
			}
			
			&:hover
			{
				background-color: rgba(0,0,0,0);
			}
		}
	}
}

//Mobile
@media screen and (max-width: 400px)
{

	div.container
	{
		padding-top: 10px !important;
	}

	header
	{
		position: relative;
		
		ul.nav
		{
			margin-left: -40px;
		
			position: absolute;
			top: 50px;
			left: 0;
			
			display: none;
			width: 100%;
			height: auto;
			text-align: center;
			
			li
			{
				background-color: $grey;
				display: block;
				
				border-top: 1px solid lighten($grey, 5);
			}
		}
		
		a.menu-toggle
		{
			display: block;
		}
	}
	
	article.work
	{
		a.thumb
		{
			display: block;
			width: 200px;
			margin: 0 auto;
		}
	}
	
	.sidebar
	{
		border-top: 1px solid lighten($grey, 50);
		padding-top: 10px;
	}
	
	nav
	{
		ul
		{
			margin-left: -40px;
		}
	}
}

//Desktop
@media screen and (max-width: 1020px)
{
	div.hero
	{
		border-radius: 0;
	}
}

//Tablet
@media screen and (max-width: 800px)
{

  div.blurb
  {
    width: auto;
    right: 20px;
  }
  
  footer
  {
    height: auto;
    text-align: center;
    
    ul.nav
    {
	    display: block !important;
    }
    
    ul.nav, p
    {
      position: relative;
      left: 0; right: 0;
      width: auto;
    }
  }
  
  div.articles, div.sidebar
  {
  	  position: relative;
  	  margin: 0 auto;
	  width: 90%;
	  max-width: 100%;
	  float: none;
	  clear: both;
  }
  
  div.container
  {
	  padding: 60px 0px;
  }
  
  div.owner
  {
	  margin-bottom: 35px;
  }
  
  header
  {
	  img
	  {
		  display: none;
	  }
  }
}

.twitter-tweet
{
	max-width: 100% !important;
	margin: 0 auto !important;
}
